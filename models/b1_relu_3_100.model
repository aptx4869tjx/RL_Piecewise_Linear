hybrid reachability
{
state var x1, x2, a, clock
setting
{
fixed steps 0.02
time 150
remainder estimation 1e-6
identity precondition
matlab octagon x1,x2
adaptive orders { min 3, max 8 }
cutoff 1e-17
precision 100
output b1_relu_3_100
max jumps 30
print on
}
modes
{
b1_sys
{
nonpoly ode
{
x1' = x2
x2' = a*x2*x2-x1
a' = 0.0
clock' = 1.0
}
inv
{
clock <= 0.2
}
}
}
jumps
{
b1_sys -> b1_sys
guard
{
clock = 0.2  -2.5 - x1 <= 0  x1 <= 0.0  -2.5 - x2 <= 0  x2 <= 0.0  }
reset
{
a' := 1.0 + -0.9812758 * x1 + -1.0 * x2
clock' := 0.0
}
interval aggregation
b1_sys -> b1_sys
guard
{
clock = 0.2  -2.5 - x1 <= 0  x1 <= 0.0  0.0 - x2 <= 0  x2 <= 2.5  }
reset
{
a' := -0.9999603 + 0.99973774 * x1 + -1.0 * x2
clock' := 0.0
}
interval aggregation
b1_sys -> b1_sys
guard
{
clock = 0.2  0.0 - x1 <= 0  x1 <= 2.5  -2.5 - x2 <= 0  x2 <= 0.0  }
reset
{
a' := 0.99993944 + 0.9993732 * x1 + -1.0 * x2
clock' := 0.0
}
interval aggregation
b1_sys -> b1_sys
guard
{
clock = 0.2  0.0 - x1 <= 0  x1 <= 2.5  0.0 - x2 <= 0  x2 <= 2.5  }
reset
{
a' := -1.0 + -0.9999235 * x1 + -1.0 * x2
clock' := 0.0
}
interval aggregation
}
init
{
b1_sys
{
x1 in [0.8, 0.9]
x2 in [0.5, 0.6]
a in [0, 0]
clock in [0.2, 0.2]
}
}
}